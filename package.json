{
  "name": "@dotbase/vue-i18next",
  "description": "i18next integration for Vue",
  "version": "1.0.4-alpha.1",
  "author": "dot.base <movebase@charite.de> (https://movebase.de)",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/dot-base/vue-i18next"
  },
  "homepage": "https://github.com/dot-base/vue-i18next",
  "bugs": {
    "url": "https://github.com/dot-base/vue-i18next/issues"
  },
  "keywords": [
    "i18n",
    "i18next",
    "vue",
    "typescript"
  ],
  "scripts": {
    "dev": "tsc --watch",
    "build": "tsc",
    "docs:dev": "vuepress dev docs --temp .temp",
    "docs:build": "vuepress build docs --temp .temp",
    "docs:publish": "gh-pages -d docs/.vuepress/dist -r https://github.com/kkuegler/vue-i18next.git"
  },
  "main": "./dist/index.js",
  "types": "./types/index.d.ts",
  "peerDependencies": {
    "i18next": ">=19",
    "vue": "^2.6.0"
  },
  "devDependencies": {
    "@vuepress/theme-vue": "^1.9.7",
    "gh-pages": "^3.2.3",
    "mkdirp": "^1.0.4",
    "ncp": "^2.0.0",
    "rimraf": "^3.0.2",
    "typescript": "^4.1.3",
    "vue": "^2.6.14",
    "vuepress": "^1.9.7"
  },
  "files": [
    "dist/**/*",
    "types/**/*"
  ]
}